  },
  "dependencies": {
    "@egjs/infinitegrid": "^3.7.0",
    "@egjs/list-differ": "^1.0.0"
  },
  "devDependencies": {
    "@egjs/build-helper": "0.0.5",
    "@types/chai": "^4.1.7",
    "@types/karma-chai": "^0.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.7.7",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@types/sinon": "^7.0.10",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^4.0.0",
