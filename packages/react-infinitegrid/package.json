    "mocha": "^6.0.2",
    "print-coveralls": "^1.2.2",
    "print-sizes": "^0.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.2",
    "tslint": "^5.20.0",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "PORT=3006 react-scripts start",
    "build": "rollup -c && npm run declaration",
