{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		"no-empty": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"no-unused-expression": false,
		"interface-name": false,
		"indent": [true, "tabs"],
		"guard-for-in": false,
		"forin": false
	}
}